ue  = true		  case "s" , "S"		    StartIt		    		  Else		    Return False		  end Select		End Function	#tag EndEvent	#tag Event		Sub Resized()		  Canvas1.Left =( me.Width-Canvas1.Width ) / 2		  		End Sub	#tag EndEvent	#tag Method, Flags = &h0		Sub DoJyanken()		  Canvas1.Backdrop = nil		  		  dim hands() As Picture		  		  hands.Append img_goo		  hands.Append img_cyoki		  hands.Append img_paa		  		  dim cSND As Sound = snd_click		  cSND.Volume = 60		  		  dim fSND As Sound = snd_ImageUploaded		  fSND.Volume = 60		  		  for i as Integer = 0 to 10		    		    dim pi As Picture		    		    for k as Integer = 0 to 2		      pi = hands( k)		      Canvas1.Backdrop = pi		      		      cSND.Play		      app.DoEvents		      DelayMBS( 0.05 )		    next		    		  next		  		  Canvas1.Backdrop = nil		  me.Refresh		  DelayMBS( 0.5 )		  		  dim rn As new Random		  Canvas1.Backdrop = hands( rn.InRange( 0,2 ) )		  		  fSND.Play		End Sub	#tag EndMethod	#tag Method, Flags = &h0		Sub StartIt()		  		  CheckBox1.Value  = False		  		  Canvas1.Backdrop = nil		  		  dim hands() As Picture		  		  hands.Append img_goo		  hands.Append img_cyoki		  hands.Append img_paa		  		  		  dim cSND As Sound = snd_click		  cSND.Volume = 60		  		  dim fSND As Sound = snd_ImageUploaded		  fSND.Volume = 60		  		  		  do		    if CheckBox1.Value then exit		    		    dim pi As Picture		    		    for k as Integer = 0 to 2		      pi = hands( k)		      Canvas1.Backdrop = pi		      		      cSND.Play		      app.DoEvents		      DelayMBS( 0.05 )		    next		    		  loop		  		  		  Canvas1.Backdrop = nil		  me.Refresh		  //DelayMBS( 0.5 )		  		  dim rn As new Random		  Canvas1.Backdrop = hands( rn.InRange( 0,2 ) )		  		  fSND.Play		  		End Sub	#tag EndMethod#tag EndWindowCode#tag Events BevelButton1	#tag Event		Sub Action()		  DoJyanken		End Sub	#tag EndEvent#tag EndEvents#tag Events BevelButton2	#tag Event		